{
  "compilerOptions": {
    "outDir": "dist/",
    "noImplicitReturns": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "alwaysStrict": true,
    "noUnusedParameters": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2021",
    "jsx": "react",
    "types": [
    ]
  },
  "include": [
    "src/*.ts",
    "src/app/*.tsx",
    "test/tests.ts"
  ],
}